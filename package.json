{
  "name": "sushi-go",
  "version": "1.0.0",
  "description": "Online version of the table top game Sushi Go",
  "devDependencies": {
    "concurrently": "5.1.0",
    "react-scripts": "^3.4.1"
  },
  "dependencies": {
    "firebase": "^7.14.2",
    "prop-types": "^15.7.2",
    "re-base": "4.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "react-transition-group": "^4.3.0",
    "sass": "^1.26.5",
    "serve": "^11.3.0",
    "typescript": "^3.8.3"
  },
  "scripts": {
    "dev": "react-scripts start",
    "start": "react-scripts start",
    "watch": "concurrently --names \"webpack, sass\" --prefix name \"npm run start\" \"npm run styles:watch\"",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "styles": "sass src/scss/style.scss public/css/main.css",
    "now-build": "npm run build && mv build dist",
    "styles:watch": "npm run styles -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arobillard/sushi-go.git"
  },
  "keywords": [
    "sushi",
    "table-top-games"
  ],
  "author": "Adam Robillard",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/arobillard/sushi-go/issues"
  },
  "homepage": "https://ar-sushi-go.netlify.app",
  "browserslist": [
    ">0.2%",
    "not ie <= 11",
    "not op_mini all"
  ]
}
